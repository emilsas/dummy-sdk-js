{
    "tags": {
      "allowUnknownTags": false
    },
    "source": {
      "include": ["src"],
      "includePattern": ".+\\.js(doc|x)?$",
      "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [
      "plugins/markdown",
      "node_modules/jsdoc-export-default-interop/dist/index"
    ],
    "opts": {
      "template": "node_modules/clean-jsdoc-theme",
      "encoding": "utf8",
      "destination": "docs/",
      "recurse": true,
      "verbose": true,
      "theme_opts": {
        "title": "My Dummy SDK",
        "theme": "dark"
      }
    },
    "templates": {
      "cleverLinks": false,
      "monospaceLinks": false
    }
  }